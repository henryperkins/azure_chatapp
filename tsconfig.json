{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "target": "es2021",
    "module": "es2021",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": [
    "static/js/**/*.js",
    "static/services/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "static/js/vendor"
  ]
}